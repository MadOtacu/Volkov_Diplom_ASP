<Page x:Class="Volkov_Diplom.PagesAdminPanel.UsersPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Volkov_Diplom.PagesAdminPanel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UsersPage">

    <Grid Background="White">
        <DataGrid x:Name="users" Background="White">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Логин" Binding="{Binding Path=login}"/>
                <DataGridTextColumn Header="Почта" Binding="{Binding Path=email}"/>
                <DataGridTextColumn Header="Результат теста" Binding="{Binding Path=resultTest}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
